{
  "version": 2,
  "cors": [
    {
      "source": "https://ai-trading-2-0-2.onrender.com",
      "headers": ["Access-Control-Allow-Origin", "Access-Control-Allow-Methods", "Access-Control-Allow-Headers"]
    },
    {
      "source": "https://ai-trading-2-0-1.onrender.com",
      "headers": ["Access-Control-Allow-Origin", "Access-Control-Allow-Methods", "Access-Control-Allow-Headers"]
    },
    {
      "source": "https://ai-trading-2-0.onrender.com",
      "headers": ["Access-Control-Allow-Origin", "Access-Control-Allow-Methods", "Access-Control-Allow-Headers"]
    },
    {
      "source": "http://localhost:3000",
      "headers": ["Access-Control-Allow-Origin", "Access-Control-Allow-Methods", "Access-Control-Allow-Headers"]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Authorization, Content-Type, X-Requested-With"
        }
      ]
    }
  ],
  "buildCommand": "echo 'No build needed for static HTML'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/pages/dashboard.html"
    },
    {
      "source": "/trade",
      "destination": "/pages/trade.html"
    },
    {
      "source": "/ml",
      "destination": "/pages/mldashboard.html"
    },
    {
      "source": "/login",
      "destination": "/pages/login.html"
    },
    {
      "source": "/subscribe",
      "destination": "/pages/subscribe.html"
    },
    {
      "source": "/register",
      "destination": "/pages/register.html"
    },
    {
      "source": "/settings",
      "destination": "/pages/settings.html"
    },
    {
      "source": "/mt5-setup",
      "destination": "/pages/mt5-setup.html"
    }
  ]
}